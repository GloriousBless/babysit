{
	"easycom": {
			"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
		},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		
		{
		    "path" : "pages/StartPage/StartPage",
		    "style" :                                                                                    
		    {
		        "navigationBarTitleText": "首页",
		        "enablePullDownRefresh": false
		    }
		    
		},
		{
			"path" : "pages/login/login",
			"style" :                                                                                    
			{
				"navigationBarTitleText": "登录",
				"enablePullDownRefresh": false
			}
			
		},
	    {
            "path" : "pages/user/user",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "个人中心",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/stastistics/stastistics",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "统计",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/Shopping/Shopping",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "商城",
                "enablePullDownRefresh": false
            }
            
        }
        
        ,{
            "path" : "pages/Square/Square",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "广场",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/Healty_diet/Healty_diet",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "健康饮食",
                "enablePullDownRefresh": false
            }
            
        }
        ,
        {
            "path" : "pages/monitor/monitor",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "监测",
                "enablePullDownRefresh": false
            }
            
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {},
	"tabBar": {
		"color": "#B9A7C2",
		"selectedColor": "#6a60a9",
		"list": [
			{
				"text": "首页",
				"pagePath": "pages/StartPage/StartPage",
				"iconPath": "static/tabBar/首页未选中.png",
				"selectedIconPath": "static/tabBar/主页.png"
			},
			{
				"text": "统计",
				"pagePath": "pages/stastistics/stastistics",
				"iconPath": "static/tabBar/统计未选中.png",
				"selectedIconPath": "static/tabBar/统计.png"
			},
			{
				"text": "商城",
				"pagePath": "pages/Shopping/Shopping",
				"iconPath": "static/tabBar/购物车未选中.png",
				"selectedIconPath": "static/tabBar/购物车-fill.png"
			},
			{
				"text": "广场",
				"pagePath": "pages/Square/Square",
				"iconPath": "static/tabBar/对话.png",
				"selectedIconPath": "static/tabBar/对话框选中.png"
			},
			{
				"text": "我的",
				"pagePath": "pages/user/user",
				"iconPath": "static/tabBar/个人中心.png",
				"selectedIconPath": "static/tabBar/个人中心选中.png"
			}
		]
		
	}
}
